{
    "serverRoot": "http://localhost:8000",
    "port": 8000,
    "dbtype": "postgres",
    "dbconfig": "postgres://postgres:[YOUR-PASSWORD]@[YOUR-HOST]:[YOUR-PORT]/postgres?sslmode=require",
    "postgres_dbconfig": "postgres://postgres:[YOUR-PASSWORD]@[YOUR-HOST]:[YOUR-PORT]/postgres?sslmode=require",
    "useSSL": false,
    "webpath": "./pack",
    "filespath": "./files",
    "telemetry": true,
    "prometheusaddress": ":9092",
    "session_expire_time": 2592000,
    "session_refresh_time": 18000,
    "localOnly": false,
    "enableLocalMode": true,
    "localModeSocketLocation": "/var/tmp/focalboard_local.socket"
}
